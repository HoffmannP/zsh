# Link to ~/.config/terminator/config
[global_config]
  handle_size = 1
  enabled_plugins = ,
  focus = system
  title_hide_sizetext = True
  title_transmit_bg_color = "#8500c8"
  inactive_color_offset = 0.47
[keybindings]
  close_window = None
  group_tab = None
  resize_left = None
  rotate_ccw = None
  move_tab_left = None
  resize_right = None
  group_all = None
  toggle_zoom = F11
  scaled_zoom = None
  resize_down = None
  full_screen = <Shift>F11
  split_vert = <Shift><Alt>Right
  ungroup_all = None
  ungroup_tab = None
  paste = <Primary>v
  cycle_next = <Primary>Tab
  go_prev = None
  close_term = <Alt>F4
  new_terminator = None
  go_next = None
  copy = <Primary>c
  hide_window = None
  reset = None
  search = None
  rotate_cw = None
  resize_up = None
  split_horiz = <Shift><Alt>Down
  move_tab_right = None
  new_window = <Primary><Shift>n
  reset_clear = None
  toggle_scrollbar = None
[profiles]
  [[default]]
    scrollbar_position = hidden
    palette = "#000000:#aa0000:#00aa00:#aa5500:#0000aa:#aa00aa:#00aaaa:#aaaaaa:#555555:#ff5555:#55ff55:#ffff55:#5555ff:#ff55ff:#55ffff:#ffffff"
    background_image = None
    foreground_color = "#00ff00"
    show_titlebar = False
    copy_on_selection = True
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
