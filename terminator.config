[global_config]
  enabled_plugins = ,
  focus = system
  handle_size = 1
  inactive_color_offset = 0.71
  suppress_multiple_term_dialog = True
  title_hide_sizetext = True
  title_transmit_bg_color = "#8500c8"
[keybindings]
  broadcast_group = None
  close_term = <Shift><Alt>w
  close_window = <Alt>F4
  copy = <Primary>c
  cycle_next = <Primary>Tab
  full_screen = <Shift>F11
  go_next = None
  go_prev = None
  group_all = None
  group_tab = None
  help = None
  hide_window = None
  move_tab_left = None
  move_tab_right = None
  new_terminator = None
  new_window = <Primary><Shift>n
  page_down = None
  page_up = None
  paste = <Primary>v
  reset = None
  reset_clear = None
  resize_down = <Primary><Shift>Down
  resize_left = <Primary><Shift>Left
  resize_right = <Primary><Shift>Right
  resize_up = <Primary><Shift>Up
  rotate_ccw = <Primary>r
  rotate_cw = None
  scaled_zoom = None
  search = None
  split_horiz = <Shift><Alt>Down
  split_vert = <Shift><Alt>Right
  toggle_scrollbar = None
  toggle_zoom = F11
  ungroup_all = None
  ungroup_tab = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.84
    background_type = transparent
    copy_on_selection = True
    font = Source Code Pro Medium 9
    cursor_color = "#00ff00"
    foreground_color = "#00ff00"
    icon_bell = False
    login_shell = True
    palette = "#000000:#aa0000:#00aa00:#aa5500:#0000aa:#aa00aa:#00aaaa:#aaaaaa:#555555:#ff5555:#55ff55:#ffff55:#5555ff:#ff55ff:#55ffff:#ffffff"
    scrollback_infinite = True
    scrollbar_position = hidden
    show_titlebar = False
    update_records = False
    use_system_font = False
    visible_bell = True
